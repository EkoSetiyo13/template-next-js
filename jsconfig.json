{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@prisma-client/*": [
        "prisma/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}